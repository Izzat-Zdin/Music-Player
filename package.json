{
	"name": "local-music-pwa-next",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"biome-check": "biome check ./src",
		"biome-fix": "biome check ./src --write",
		"format-prettier-fix": "prettier --write ./src",
		"compile-i18n": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@inlang/paraglide-js": "^1.11.3",
		"@inlang/paraglide-vite": "^1.2.77",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.9.0",
		"@sveltejs/vite-plugin-svelte": "5.0.1",
		"@tailwindcss/vite": "4.0.0-beta.4",
		"prettier": "^3.4.1",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte": "^5.2.12",
		"svelte-check": "^4.1.0",
		"tailwindcss": "4.0.0-beta.4",
		"terser": "^5.36.0",
		"typescript": "^5.7.2",
		"unplugin-auto-import": "^0.18.6",
		"vite": "^6.0.1"
	},
	"dependencies": {
		"@a11y/focus-trap": "^1.0.5",
		"@floating-ui/dom": "^1.6.12",
		"@material/material-color-utilities": "^0.3.0",
