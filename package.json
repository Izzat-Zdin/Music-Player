{
	"name": "local-music-pwa-next",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "biome lint ./src",
		"lint-fix": "biome check ./src --apply",
		"format-prettier-fix": "prettier --write ./src",
		"format": "biome format ./src",
		"format-fix": "biome format ./src --write",
		"compile-i18n": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.1",
		"@inlang/paraglide-js": "^1.9.1",
		"@inlang/paraglide-js-adapter-vite": "^1.2.40",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.10",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.3",
		"@unocss/postcss": "^0.61.0",
		"@unocss/svelte-scoped": "^0.61.0",
		"autoprefixer": "^10.4.19",
		"nanoid": "^5.0.7",
		"postcss": "^8.4.38",
		"postcss-nesting": "^12.1.5",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.4",
		"svelte": "5.0.0-next.154",
		"svelte-check": "^3.8.0",
		"terser": "^5.31.1",
		"typescript": "^5.4.5",
