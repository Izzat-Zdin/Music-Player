{
	"name": "local-music-pwa-next",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "biome lint ./src",
		"lint-fix": "biome check ./src --apply",
		"format": "biome format ./src",
		"format-fix": "biome format ./src --write",
		"generate-tw-colors": "node scripts/generate-tw-colors.js",
		"compile-i18n": "paraglide-js compile --project ./project.inlang",
		"postinstall": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@biomejs/biome": "1.4.1",
		"@inlang/paraglide-js": "1.0.0-prerelease.22",
		"@inlang/paraglide-js-adapter-vite": "^1.0.3",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@unocss/postcss": "^0.58.0",
		"@unocss/svelte-scoped": "^0.58.0",
		"autoprefixer": "^10.4.16",
		"nanoid": "^5.0.4",
		"postcss": "^8.4.32",
		"postcss-nesting": "^12.0.2",
		"svelte": "5.0.0-next.26",
		"svelte-check": "^3.6.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"unocss": "^0.58.0",
		"unplugin-auto-import": "^0.17.2",
		"vite": "^5.0.10"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
		"@material/material-color-utilities": "^0.2.7",
		"@motionone/svelte": "^10.16.4",
		"@types/dom-navigation": "^1.0.3",
		"@types/dom-view-transitions": "^1.0.4",
		"@types/offscreencanvas": "^2019.7.3",
		"@types/wicg-file-system-access": "^2023.10.4",
		"@unocss/reset": "^0.58.0",
		"idb": "^8.0.0",
		"motion": "^10.16.4",
		"music-metadata": "^7.14.0",
		"svelte-portal": "^2.2.0",
		"tiny-invariant": "^1.3.1",
		"weak-lru-cache": "^1.2.2"
	},
	"engines": {
		"node": "20.6.1"
	},
	"packageManager": "pnpm@8.7.4"
}
