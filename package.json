{
	"name": "local-music-pwa-next",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "biome lint ./src",
		"lint-fix": "biome check ./src --apply",
		"format": "biome format ./src",
		"format-fix": "biome format ./src --write",
		"compile-i18n": "paraglide-js compile --project ./project.inlang",
		"postinstall": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.1",
		"@inlang/paraglide-js": "^1.1.1",
		"@inlang/paraglide-js-adapter-vite": "^1.2.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@unocss/postcss": "^0.58.3",
		"@unocss/svelte-scoped": "^0.58.3",
		"autoprefixer": "^10.4.16",
		"nanoid": "^5.0.4",
		"postcss": "^8.4.33",
		"postcss-nesting": "^12.0.2",
		"prettier": "^3.2.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "5.0.0-next.35",
		"svelte-check": "^3.6.3",
		"terser": "^5.26.0",
		"typescript": "^5.3.3",
		"unocss": "^0.58.3",
		"unplugin-auto-import": "^0.17.3",
		"vite": "^5.0.11"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.5.4",
		"@material/material-color-utilities": "^0.2.7",
		"@tanstack/svelte-virtual": "^3.0.1",
		"@types/dom-navigation": "^1.0.3",
		"@types/dom-view-transitions": "^1.0.4",
		"@types/wicg-file-system-access": "^2023.10.4",
		"@unocss/reset": "^0.58.3",
		"buffer-lite": "^2.1.0",
		"idb": "^8.0.0",
		"motion": "^10.17.0",
		"music-metadata": "^7.14.0",
		"roving-ux": "^1.0.5",
		"tiny-invariant": "^1.3.1",
		"weak-lru-cache": "^1.2.2"
	},
	"engines": {
		"node": "20.10.0"
	},
	"packageManager": "pnpm@8.13.1"
}
