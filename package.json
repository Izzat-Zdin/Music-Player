{
	"name": "local-music-pwa-next",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"biome-check": "biome check ./src",
		"lint": "biome lint ./src",
		"lint-fix": "biome check ./src --apply",
		"format-prettier-fix": "prettier --write ./src",
		"format": "biome format ./src",
		"format-fix": "biome format ./src --write",
		"compile-i18n": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@inlang/paraglide-js": "^1.11.2",
		"@inlang/paraglide-vite": "^1.2.74",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.5",
		"@unocss/postcss": "0.58.9",
		"@unocss/preset-uno": "0.58.9",
		"@unocss/vite": "0.58.9",
		"autoprefixer": "^10.4.19",
		"nanoid": "^5.0.7",
		"postcss": "^8.4.40",
		"postcss-nesting": "^12.1.5",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "5.0.0-next.201",
		"svelte-check": "^3.8.4",
		"terser": "^5.31.3",
		"typescript": "^5.5.4",
		"unplugin-auto-import": "^0.18.2",
		"vite": "^5.3.5"
	},
	"dependencies": {
		"@a11y/focus-trap": "^1.0.5",
		"@floating-ui/dom": "^1.6.8",
		"@material/material-color-utilities": "^0.3.0",
		"@tanstack/svelte-virtual": "^3.8.3",
		"@types/dom-navigation": "^1.0.3",
		"@types/dom-view-transitions": "^1.0.5",
		"@types/wicg-file-system-access": "^2023.10.5",
		"@unocss/reset": "0.58.9",
		"idb": "^8.0.0",
		"motion": "^10.18.0",
		"music-metadata": "^10.0.0",
		"tiny-invariant": "^1.3.3",
		"valibot": "^0.36.0",
		"weak-lru-cache": "^1.2.2"
	},
	"engines": {
		"node": "20.11.1"
	},
	"packageManager": "pnpm@9.6.0"
}
